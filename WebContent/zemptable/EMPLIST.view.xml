<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zemptable.EMPLIST" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{m3>title}">
	        <Table items="{/company/empInfo/element}">
	          <columns>	            
	             <Column>
	                <header>
	                    <Text text="{m3>name}"/>
	                </header>
	             </Column>
	             
	             <Column>	              
	                   <Text text="{m3>role}"/>	               
	             </Column>
	             
	             <Column>
	                <Text text="{m3>dept}"/>
	             </Column>	             
	          </columns>
	          <items>
	             <ColumnListItem>
	                 <cells>
	                    <Text text="{name}"/>
	                    <Text text="{role}"/>
	                    <Text text="{dept}"/>
	                 </cells>
	             </ColumnListItem>
	          </items>
	        </Table>
		
	</Page>
</core:View>